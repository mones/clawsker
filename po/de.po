msgstr "Syntax:\n"

#: ../clawsker:1928
msgid "  clawsker [options]\n"
msgstr "  clawsker [optionen]\n"

#: ../clawsker:1929
msgid "Options:\n"
msgstr "Optionen:\n"

#: ../clawsker:1930
msgid "  -h|--help                        Prints this help screen.\n"
msgstr "  -h|--help                        Zeigt diese Hilfe an.\n"

#: ../clawsker:1931
msgid "  -v|--version                     Prints version infos.\n"
